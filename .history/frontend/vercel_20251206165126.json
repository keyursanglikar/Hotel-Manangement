{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "http://localhost:4000",
    "VITE_PRODUCTION_API_BASE_URL":" https://hotel-manangement.onrender.com/"
  }
}